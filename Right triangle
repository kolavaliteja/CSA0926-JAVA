import java.util.*;
class right_triangle
{
    public static void main(String[] args) 
      {
        int r= 5;
        int n= 1;
        for(int i=1;i<=r;i++) 
          { 
            for(int j=1;j<=n;j++)
           {
            System.out.print("* ");
           }
          n++;
           System.out.println();
      }}
}
